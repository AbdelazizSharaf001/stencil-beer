{
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "/googlePlaces",
        "function": "googlePlaces"
      },
      {
        "source": "/googleGeocode",
        "function": "googleGeocode"
      },
      {
        "source": "/vision",
        "function": "vision"
      },
      {
        "source": "/bars/**",
        "destination": "/index.html"
      },
      {
        "source": "/beers/**",
        "destination": "/index.html"
      },
      {
        "source": "/main",
        "destination": "/index.html"
      },
      {
        "source": "/profile",
        "destination": "/index.html"
      },
      {
        "source": "/users/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}

