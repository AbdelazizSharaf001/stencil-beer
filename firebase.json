{
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "/googlePlaces",
        "function": "googlePlaces"
      },
      {
        "source": "/googleGeocode",
        "function": "googleGeocode"
      },
      {
        "source": "/vision",
        "function": "vision"
      },
      {
        "source": "/bars/**",
        "destination": "/index.html"
      },
      {
        "source": "/beers/**",
        "destination": "/index.html"
      },
      {
        "source": "/main",
        "destination": "/index.html"
      },
      {
        "source": "/profile",
        "destination": "/index.html"
      },
      {
        "source": "/users/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</build/app.css>;rel=preload;as=style,</build/app/app.qqcm3h1j.js>;rel=preload;as=script,</build/app/pxisnwof.js>;rel=preload;as=script,</build/app/ndcx1naq.js>;rel=preload;as=script,</build/app/lunprpvl.js>;rel=preload;as=script,</build/app/5e3mm8py.js>;rel=preload;as=script"
          }
        ]
     }
    ]
  }
}

