{
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "/googlePlaces", 
        "function": "googlePlaces"
      },
      {
        "source": "/googleGeocode",
        "function": "googleGeocode"
      },
      {
        "source": "/bars/**",
        "destination": "/index.html"
      },
      {
        "source": "/beers/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</build/app/app.jakzatqq.js>;rel=preload;as=script,</build/app/a1vsmuko.js>;rel=preload;as=script,</build/app/vqdmmwyk.js>;rel=preload;as=script,</build/app/5e3mm8py.js>;rel=preload;as=script,</build/app/5piuuqes.js>;rel=preload;as=script,</images/smaller-beers.jpeg>;rel=preload;as=image,</images/smaller-bars.jpeg>;rel=preload;as=image"
          }
        ]
     }
    ]
  }
}

