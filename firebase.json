{
  "hosting": {
    "public": "www",
    "rewrites": [
      {
        "source": "/googlePlaces", 
        "function": "googlePlaces"
      },
      {
        "source": "/googleGeocode",
        "function": "googleGeocode"
      },
      {
        "source": "/bars/**",
        "destination": "/index.html"
      },
      {
        "source": "/beers/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "sw.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "/",
        "headers": [
          {
            "key": "Link",
            "value": "</build/app/app.eujcqoer.js>;rel=preload;as=script,</build/app/c398ps6m.js>;rel=preload;as=script,</build/app/mfk22ued.js>;rel=preload;as=script,</build/app/lc3eh6tv.js>;rel=preload;as=script,</build/app/wawblzzc.js>;rel=preload;as=script,</images/beers.jpeg>;rel=preload;as=image,</images/bars.jpeg>;rel=preload;as=image"
          }
        ]
     }
    ]
  }
}

