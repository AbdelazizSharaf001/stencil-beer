{
  "name": "@stencil/starter",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Starter App",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "stencil build --stats",
    "dev": "sd concurrent \"stencil build --dev --stats --watch\" \"stencil-dev-server\" ",
    "functionDev": "stencil build --dev --watch",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@ionic/core": "0.0.3",
    "@stencil/core": "0.6.13",
    "@stencil/router": "0.0.25",
    "firebase": "4.8.2",
    "greenlet": "^0.1.2",
    "st-img": "^0.3.0"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.2",
    "@stencil/utils": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-starter.git"
  },
